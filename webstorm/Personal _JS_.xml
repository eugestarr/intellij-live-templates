<templateSet group="Personal [JS]">
  <template name="log" value="console.log($END$);" description="Inserts console.log()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="req" value="var $VAR$ = require('$VAR$');" description="inserts requre with var to assign" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="module" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="time" value="console.time(&quot;$END$$A$&quot;);&#10;console.timeEnd(&quot;$A$&quot;);" description="inserts debug time and timeend" toReformat="false" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>